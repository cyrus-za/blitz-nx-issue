{
  "version": 2,
  "projects": {
    "blitz-app": {
      "root": "apps/blitz-app",
      "sourceRoot": "apps/blitz-app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/blitz-app",
            "outputPath": "dist/apps/blitz-app"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "blitz-app:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "blitz-app:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "blitz-app:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/blitz-app/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/blitz-app"
          ],
          "options": {
            "jestConfig": "apps/blitz-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "blitz-app-e2e": {
      "root": "apps/blitz-app-e2e",
      "sourceRoot": "apps/blitz-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/blitz-app-e2e/cypress.json",
            "tsConfig": "apps/blitz-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "blitz-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "blitz-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/blitz-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "blitz-app"
}